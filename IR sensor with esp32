int irSensorPin = 18; 
int ledPin = 19;    
void setup() {
  Serial.begin(9600);
  pinMode(irSensorPin, INPUT); 
  pinMode(ledPin, OUTPUT);     
}

void loop() {
  int sensorValue = digitalRead(irSensorPin);
  if (sensorValue == LOW) { 
    digitalWrite(ledPin, HIGH); 
    Serial.println("Object Detected!");
    delay(500);
  } else { 
    digitalWrite(ledPin, LOW); 
    Serial.println("No Object");
    delay(500);
  }
