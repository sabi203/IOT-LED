#include <LiquidCrystal.h>
LiquidCrystal lcd(22,21,19,18,5,17);

void setup() {
  // put your setup code here, to run once:
  lcd.begin(16,2);
}

void loop() {
  // put your main code here, to run repeatedly:
  lcd.setCursor(0,0);
  lcd.print("WELCOME");
  lcd.setcursor(0,1);
  lcd.print("ALL");
  
  delay(10); // this speeds up the simulation
}
